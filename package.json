{
     "name": "wptheme",
     "version": "1.0.0",
     "description": "Create a Base theme",
     "main": "webpack.mix.js",
     "scripts": {
          "compile:sass": "node-sass wpTheme-child/assets/src/sass/style.scss wpTheme-child/assets/dist/css/style.css -w",
          "dev": "npm run development",
          "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
          "watch": "npm run development -- --watch",
          "prod": "npm run production",
          "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
          "lint": "eslint \"./**/*.js\""
     },
     "repository": {
          "type": "git",
          "url": "git+https://github.com/vickycampo/wpTheme.git"
     },
     "author": "",
     "license": "ISC",
     "bugs": {
          "url": "https://github.com/vickycampo/wpTheme/issues"
     },
     "homepage": "https://github.com/vickycampo/wpTheme#readme",
     "dependencies": {
          "bootstrap": "^4.3.1",
          "install": "^0.13.0",
          "npm": "^6.10.1"
     },
     "devDependencies": {
          "cross-env": "^5.2.0",
          "laravel-mix": "^4.1.2",
          "node-sass": "^4.12.0",
          "webpack": "^4.39.1",
          "webpack-cli": "^3.3.6"
     }
}
